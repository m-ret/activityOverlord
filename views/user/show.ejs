<div class="container">
	<h1><%=	user.name	%></h1>
		<% if(user.admin) { %>
			<td><img src="/images/angular.png" alt=""></td>
		<% } else { %>
			<td><img src="/images/js.png" alt=""></td>
		<% } %>
	<h3><%=	user.title	%></h3>
	<hr>
	<h3>Contact: <%=	user.email	%></h3>
	<a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id %>">Edit</a>
</div>
